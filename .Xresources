#define background_opacity 95
#include ".config/xres/base16-xresources/xresources/base16-google-dark-256.Xresources"
#define leFont xos4 terminus
#define rofibg base00
#define rofibt base00
#define rofifg base05
#define rofihl base07
#define rofi1 base07
#define rofi2 base0c
#define rofi3 base0E
#define rofi4 base0D

URxvt.lineSpace: 12
URxvt.keysym.C-Up: font-size:increase
URxvt.keysym.C-Down: font-size:decrease
URxvt.font-size.step: 1
URxvt.cursorBlink: false
URxvt.cursorUnderline: true
URxvt.internalBorder: 20
URxvt.externalBorder: 0
URxvt.perl-ext-common: default,font-size
urxvt.font: -*-terminus-medium-r-*-*-14-*-*-*-*-*-iso10646-*
urxvt.depth: 32
urxvt.scrollBar: false

! State:           bg,     fg,     bgalt,  hlbg,   hlfg
rofi.color-normal: rofibt, rofifg, rofibt, rofibt, rofi2
rofi.color-urgent: rofibt, rofifg, rofibt, rofibt, rofi3
rofi.color-active: rofibt, rofifg, rofibt, rofibt, rofi4
rofi.color-window: rofibg, rofi1, rofibt, rofibt
rofi.font: leFont 14
