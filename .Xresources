urxvt --help 2>&1| sed -n '/:  /s/^ */URxvt*/gp' >> ~/.Xresources
URxvt.perl-ext-common: default,matcher,font-size,urxvt-colors
urxvt.font:                xft:Roboto Mono for Powerline:size=12,xft:DejaVu Sans Mono for Powerline:size=12,xft:FontAwesome:size=12
