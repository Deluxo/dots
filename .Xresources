#define background_opacity 95
#include ".config/xres/base16-xresources/xresources/base16-google-dark-256.Xresources"
#define leFont xos4 terminus
#define rofibg #DD000000
#define rofibt #00000000
#define rofifg color10
#define rofihl #98C379
#define rofi1 #98C379
#define rofi2 #E5C07B
#define rofi3 #61AFEF
#define rofi4 #56B6C2

URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
