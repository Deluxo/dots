urxvt.boldItalicFont: -*-roboto mono-bold-i-*-*-14-*-*-*-*-*-iso10646-*
urxvt.depth: 32
urxvt.scrollBar: false
urxvt.background: [75]backgr

! State:           'bg'    'fg'    'bgalt' 'hlbg'  'hlfg'
rofi.color-normal: backgr, base0E, backgr, backgr, base0F
