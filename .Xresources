URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.font-size.step: 1
URxvt.cursorBlink: true
URxvt.cursorUnderline: true
URxvt.internalBorder: 0
URxvt.externalBorder: 0
urxvt --help 2>&1| sed -n '/:  /s/^ */URxvt*/gp' >> ~/.Xresources
URxvt.perl-ext-common: default,font-size,urxvt-colors
urxvt.font:                xft:Roboto Mono for Powerline:size=12,xft:DejaVu Sans Mono for Powerline:size=12,xft:FontAwesome:size=12,xft:Symbola:size=12
urxvt.boldFont:            xft:Roboto Mono for Powerline:size=12:bold,xft:DejaVu Sans Mono for Powerline:size=12,xft:FontAwesome:size=12,xft:Symbola:size=12
urxvt.italicFont:          xft:Roboto Mono for Powerline:size=12:italic,xft:DejaVu Sans Mono for Powerline:size=12,xft:FontAwesome:size=12,xft:Symbola:size=12
urxvt.boldItalicFont:      xft:Roboto Mono for Powerline:size=12:bold:italic,xft:DejaVu Sans Mono for Powerline:size=12,xft:FontAwesome:size=12,xft:Symbola:size=12
urxvt.depth: 32
urxvt.scrollBar: false

