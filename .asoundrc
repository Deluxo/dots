ctl.equal {
	type equal;
}

pcm.microphone {
	type dsnoop;
	ipc_key 1024;
	slave {
		pcm "hw:0,0,0";
		channels 1;
		rate 48000;
	}
}

pcm.equalizer {
	type equal;
	slave.pcm "plug:dmix";
}

pcm.!default {
	type asym;
	playback.pcm {
		type plug;
		slave.pcm "equalizer";
	}
	capture.pcm {
		type plug;
		slave.pcm "microphone";
	}
}
